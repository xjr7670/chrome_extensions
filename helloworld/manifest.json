{
    "manifest_version": 2,
    "name": "Hello World",
    "version": "1.0",
    "description": "A hello world extension",
    "icons": {
        "128": "./images/icon128.png",
        "48": "./images/icon48.png",
        "16": "./images/icon16.png"
    },
    "browser_action": {
        "default_icon": "./images/icon16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["./js/popup.js"]
    }
}